{
  "name": "@tomekf/gqlite",
  "version": "0.0.0-beta.9",
  "description": "Lightweight GraphQL requests",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build",
    "prepare": "npm run build",
    "build": "babel src --out-dir dist --source-maps --ignore test.js",
    "eslint-src": "eslint src/**/*.js"
  },
  "repository": "tomek-f/gqlite",
  "files": [
    "dist",
    "src",
    "*.md",
    "yarn.lock",
    ".eslintignore",
    ".eslintrc.js",
    ".babelrc",
    ".gitignore"
  ],
  "readmeFilename": "README.md",
  "keywords": [
    "graphql",
    "gql"
  ],
  "browserslist": [
    "> 1%",
    "last 2 major versions",
    "last 5 chrome major versions",
    "last 5 firefox major versions",
    "last 5 opera major versions",
    "last 5 edge major versions",
    "not dead",
    "not ie 0-11",
    "not ie_mob 0-11",
    "not op_mini all"
  ],
  "author": "Tomek Fijo≈Ç <tomekfijol@gmail.com> (http://tomekf.pl/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
